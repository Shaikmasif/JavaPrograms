
public class ReverseWordsOfString {
	String reverseString(String s)
	{
		String str[]=s.split(" ");
		for(int i=0;i<=str.length-1;i++)
		{
			reverseWord(str[i]);
		}
	}

	String reverseWord(String s)
	{
		char ch[]=s.toCharArray();
		char ch2[]=new char[ch.length];
		int j=ch.length-1;
		for(int i=0;i<=ch.length-1;i++)
		{
			ch2[j]=ch[i];
			j--;
		}
	}
	return null;

}
